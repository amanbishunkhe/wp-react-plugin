{
	"name": "first-block",
	"version": "0.1.0",
	"description": "Example block scaffolded with Create Block tool.",
	"author": "The WordPress Contributors",
	"license": "GPL-2.0-or-later",
	"main": "build/index.js",
	"scripts": {
		"build:admin": "wp-scripts build --webpack-src-dir=assets/resource/admin --output-path=assets/build/admin",
		"build:public": "wp-scripts build --webpack-src-dir=assets/resource/public --output-path=assets/build/public",
		"build": "npm-run-all build:admin build:public",
		"format": "wp-scripts format",
		"lint:css": "wp-scripts lint-style",
		"lint:js": "wp-scripts lint-js",
		"packages-update": "wp-scripts packages-update",
		"plugin-zip": "wp-scripts plugin-zip",
		"start:admin": "wp-scripts start --webpack-src-dir=assets/resource/admin --output-path=assets/build/admin",
		"start:public": "wp-scripts start --webpack-src-dir=assets/resource/public --output-path=assets/build/public",
		"start": "npm-run-all --parallel start:admin start:public"
	},
	"devDependencies": {
		"@wordpress/scripts": "^30.14.1",
		"npm-run-all": "^4.1.5"
	}
}
